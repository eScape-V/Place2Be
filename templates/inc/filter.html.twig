{{ form_start(form) }}
<div class="container">
    <div class="row bg-secondary text-white" style="width: 69em; margin-top: 0.5em">

        <div class="col-md-3">
            <div>
                {{ form_row(form.campus) }}
            </div>
            <div>
                <h7>Le nom de la sortie contient:</h7>
                {{ form_row(form.q) }}
            </div>
        </div>
        <div class="col">
            <div>
                {{ form_row(form.dateMin) }}
            </div>
        </div>
        <div class="col">
            <div>
            {{ form_row(form.dateMax) }}
            </div>
        </div>
        <div class="col-md-3">
            {{ form_row(form.organisateur) }}
        </div>
        <div class="col">
            <button type="submit" class="btn btn-primary bg-primary" style="margin-top: 1.5em">Chercher</button>
        </div>
    </div>
</div>
{{ form_end(form) }}